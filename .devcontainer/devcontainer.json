{
  "name": "WjecDocumentScraper",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-azuretools.vscode-docker",
        "mechatroner.rainbow-csv"
      ],
      "settings": {
        "python.defaultInterpreterPath": "${containerWorkspaceFolder}/.venv/bin/python"
        "python.defaultInterpreterPath": "${containerWorkspaceFolder}/.venv/bin/python"
      }
    }
  },
  "containerEnv": {
    "JAVA_TOOL_OPTIONS": "-Djava.awt.headless=true"
  },
  "postCreateCommand": "cd /workspaces/WjecDocumentScraper && bash /workspaces/WjecDocumentScraper/.devcontainer/post_create.sh",
  "remoteUser": "vscode",
  "workspaceFolder": "/workspaces/WjecDocumentScraper"
}
